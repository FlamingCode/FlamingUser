{
    "name": "flaming-code/flaming-user",
    "description": "A module for managing users and authentication. Uses Doctrine.",
    "type": "library",
    "license": "GPL-2.0",
    "homepage": "https://github.com/FlamingCode/FlamingUser",
    "keywords": [
        "flaminguser",
        "module",
        "zf2",
        "zend"
    ],
    "authors": [
        {
            "name": "Flemming Andersen",
            "email": "flemming@flamingcode.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3",
        "zendframework/zend-loader": "~2.1",
        "zendframework/zend-modulemanager": "~2.1",
        "zendframework/zend-mvc": "~2.1",
        "zendframework/zend-servicemanager": "~2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7",
        "zendframework/zendframework": "~2.1"
    },
    "suggest": {
        "doctrine/doctrine-module": "If you want to use Doctrine",
        "doctrine/doctrine-orm-module": "If you want to use Doctrine"
    },
    "autoload": {
        "psr-0": {
            "FlamingUser\\": "src/"
        },
        "classmap": [
            "./"
        ]
    }
}
